<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ServletGinf</servlet-name>
        <servlet-class>servlet.ServletGinf</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletDeletar</servlet-name>
        <servlet-class>servlet.ServletDeletar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>servlet.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletPostagem</servlet-name>
        <servlet-class>servlet.ServletPostagem</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>servlet.Logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletPostagem</servlet-name>
        <url-pattern>/ServletPostagem</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletGinf</servlet-name>
        <url-pattern>/ServletGinf</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletDeletar</servlet-name>
        <url-pattern>/ServletDeletar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
